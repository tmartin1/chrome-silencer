{
  "manifest_version": 2,

  "name": "Chrome Silencer",
  "description": "Prevents embedded video/audio from auto-playing.",
  "version": "1.0",

  "icons": {
    "128": "icon_512px.png"
  },

  "content_scripts": [
    {
      "matches": ["*.*"],
      "js": ["run.js"]
    }
  ],

  "permissions": [
    "activeTab", "tabs"
  ]
}
